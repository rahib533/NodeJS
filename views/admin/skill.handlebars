
{{#each skill}}
  <form method="post" action="/admin/skill">
    <input type="text" name="name" placeholder="name" value="{{name}}" required >
    <br> <br>
    <input type="text" name="className" placeholder="className" value="{{className}}" required >
    <br> <br>
    <input type="number" max="10" name="rate" placeholder="rate" value="{{rate}}" required >
    <br> <br>
    <input type="hidden" name="guidId" value={{id}} >
    <input type="submit" value="Edit et" >
    <a href="/admin/skillDelete/{{id}}">Sil</a>
</form>
<br> 
{{/each}}

