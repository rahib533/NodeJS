 <div class="container-fluid p-0">
 <section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
        <div class="my-auto">
          <h1 class="mb-0">{{about.firstName}} {{about.letter}}
            <span class="text-primary">{{about.lastName}}</span>
          </h1>
          <div class="subheading mb-5">{{contact.adress}} · {{contact.phone}} ·
            <a href="mailto:{{contact.email}}">{{contact.email}}</a>
          </div>
          <p class="mb-5">{{about.description}}</p>
          <ul class="list-inline list-social-icons mb-0">
            <li class="list-inline-item">
              <a href="{{contact.facebook}}">
                <span class="fa-stack fa-lg">
                  <i class="fa fa-circle fa-stack-2x"></i>
                  <i class="fa fa-facebook fa-stack-1x fa-inverse"></i>
                </span>
              </a>
            </li>
            <li class="list-inline-item">
              <a href="{{contact.linkedin}}">
                <span class="fa-stack fa-lg">
                  <i class="fa fa-circle fa-stack-2x"></i>
                  <i class="fa fa-linkedin fa-stack-1x fa-inverse"></i>
                </span>
              </a>
            </li>
            <li class="list-inline-item">
              <a href="{{contact.github}}">
                <span class="fa-stack fa-lg">
                  <i class="fa fa-circle fa-stack-2x"></i>
                  <i class="fa fa-github fa-stack-1x fa-inverse"></i>
                </span>
              </a>
            </li>
          </ul>
        </div>
      </section>

      <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="experience">
        <div class="my-auto">
          <h2 class="mb-5">Experience</h2>

          {{#each experience}}
          <div class="resume-item d-flex flex-column flex-md-row mb-5">
            <div class="resume-content mr-auto">
              <h3 class="mb-0">{{role}}</h3>
              <div class="subheading mb-3">{{location}}</div>
              <p>{{description}}</p>
            </div>
            <div class="resume-date text-md-right">
              <span class="text-primary">{{beginDate}} - {{endDate}}</span>
            </div>
          </div>
          {{/each}}
        </div>

      </section>

      <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="education">
        <div class="my-auto">
          <h2 class="mb-5">Education</h2>
          {{#each education}}
          <div class="resume-item d-flex flex-column flex-md-row mb-5">
            <div class="resume-content mr-auto">
              <h3 class="mb-0">{{location}}</h3>
              <div class="subheading mb-3">{{degree}}</div>
              <div>{{specality}}</div>
              <p>Grade: {{grade}}</p>
            </div>
            <div class="resume-date text-md-right">
              <span class="text-primary">{{beginDate}} - {{endDate}}</span>
            </div>
          </div>
          {{/each}}
        </div>
      </section>

      <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="skills">
        <div class="my-auto">
          <h2 class="mb-5">Skills</h2>

          <div class="subheading mb-3">Programming Languages &amp; Tools</div>
          <ul class="list-inline list-icons">
            {{#each skill}}
            <li class="list-inline-item">
              <i class="{{className}}"></i>
            </li>
            {{/each}}
          </ul>

          <div class="subheading mb-3">Workflow</div>
          <ul class="fa-ul mb-0">
            {{#each workflow}}
            <li>
              <i class="fa-li fa fa-check"></i>
              {{description}}
            </li>
            {{/each}}
          </ul>
        </div>
      </section>

      <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="interests">
        <div class="my-auto">
          <h2 class="mb-5">Interests</h2>
          {{#each interest}}
          <p class="mb-0">{{description}}</p> <br>
          {{/each}}
        </div>
      </section>

      <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="awards">
        <div class="my-auto">
          <h2 class="mb-5">Awards &amp; Certifications</h2>
          <ul class="fa-ul mb-0">
            {{#each award}}
            <li>
              <i class="fa-li fa fa-trophy text-warning"></i>
              {{description}}
            </li>
            {{/each}}
          </ul>
        </div>
      </section>
      </div>